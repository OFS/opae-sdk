          <name>OPAE_HASH_MAP_UNIQUE_KEYSPACE</name>
          <initializer>= (1u &lt;&lt; 0)</initializer>
          <briefdescription>
          </briefdescription>
          <detaileddescription>
          </detaileddescription>
